<splitter_config>
    <version>0.40</version>
    <data_type>encoded</data_type>
    <fft_len>256</fft_len>
    <ifft_len>1</ifft_len>
    <filter>polyphase</filter>
    <window>hanning</window>
    <samples_per_wu>1048576</samples_per_wu>
